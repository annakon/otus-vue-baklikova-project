{
  "name": "otus-vue-baklikova-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:fix": "prettier --write . && eslint . --fix src --ext .js,.ts",
    "format": "prettier --write src",
    "prepare": "husky install"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "vite": "^4.3.4",
    "vitest": "^0.31.0"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs}": "eslint --cache --fix",
    "*.src": "prettier --write"
  }
}
